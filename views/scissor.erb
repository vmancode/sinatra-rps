



  <h1>Welcome to your Sinatra App!</h1>
  <p>We played scissor!</p>
  <p>They played <%= @comp_move %>!</p>

  <% if @outcome == "won" %>
  <p style="color: green">We <%= @outcome %>!</p>
  <% elsif @outcome == "lost" %>
  <p style="color: red">We <%= @outcome %>!</p>
  <% else @outcome == "tied" %>
  <p style="color: black">We <%= @outcome %>!</p>
  <% end %>

